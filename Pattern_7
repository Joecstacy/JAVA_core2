import java.util.Scanner;
public class Lec5q5 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int n=scan.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=n-1;j>=i;j--) {
				System.out.print("  ");
			}System.out.print("* * * * *");
			System.out.println();
		}
	}

}
