import java.util.*;
public class Lec5q2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan=new Scanner(System.in);
		int row=scan.nextInt();
		for(int i=1;i<=row;i++) {
			for(int j=1;j<=i;j++) {
				System.out.print(j);
        //	System.out.print(i); this will give anothe output
			}
			System.out.println();
		}
	}

}
